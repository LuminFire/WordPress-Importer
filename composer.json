	"require"           : {
		"php"                 : ">=5.2",
		"composer/installers" : "~1.0"
	},
	"autoload"          : { },
	"autoload-dev"      : { },
	"extra"             : {
		"branch-alias"    : {
			"dev-dev" : "0.1.x-dev"
