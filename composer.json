{
	"name"              : "humanmade/wordpress-importer",
	"description"       : "WordPress importer plugin",
	"license"           : "MIT",
	"authors"           : [
		{
			"name"     : "Ryan McCue",
			"email"    : "me@ryanmccue.info",
