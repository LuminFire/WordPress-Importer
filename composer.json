	"license"           : "MIT",
	"authors"           : [
		{
			"name"     : "Ryan McCue",
			"email"    : "me@ryanmccue.info",
			"homepage" : "http://ryanmccue.info/"
		},
		{
			"name"     : "Franz Josef Kaiser",
			"email"    : "wecodemore@gmail.com",
			"homepage" : "http://unserkaiser.com"
		}
	],
	"type"              : "wordpress-plugin",
	"minimum-stability" : "dev",
